    <m:titulo>Caracterización del transportador de serotonina humano en células Caco-2: Estudio de los mecanismos de regulación fisiológica</m:titulo>
    <m:creador>
      <rdfs:Resource>
        <m:nombrePersona>Alcalde Herrero, Ana Isabel</m:nombrePersona>
        <rdf:type rdf:resource="http://github.com/vpec/Recoreon/Persona"/>
