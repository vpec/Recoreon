    <m:titulo>MC-Spy. Un sistema para la explotación de la información histórica de operaciones realizadas en banca electrónica</m:titulo>
    <rdf:type rdf:nodeID="A1"/>
    <m:creador>
      <rdfs:Resource>
        <m:nombrePersona>Abadía Zárate, Santiago</m:nombrePersona>
        <rdf:type rdf:resource="http://github.com/vpec/Recoreon/Persona"/>
      </rdfs:Resource>
