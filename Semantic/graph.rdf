    <m:titulo>Nuevas variables canónicas para la integración analítica del problema principal-ecuatorial del satélite artificial</m:titulo>
    <rdf:type rdf:nodeID="A1"/>
    <m:formato rdf:resource="http://github.com/vpec/Recoreon/skos#pdf"/>
