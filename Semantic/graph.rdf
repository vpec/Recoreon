    <m:titulo>Desarrollo e implementación de un sistema de gestión de la calidad en un laboratorio de referencia “Unidad de metales”:  Acreditación según la UNE-EN ISO 15.189</m:titulo>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <m:creador>
      <rdfs:Resource>
        <m:nombrePersona>Izquierdo Álvarez, Silvia</m:nombrePersona>
        <rdf:type rdf:resource="http://github.com/vpec/Recoreon/Persona"/>
