    <m:titulo>Desarrollo de un agente virtual multimodal para dispositivos móviles</m:titulo>
    <m:idioma rdf:datatype="http://www.w3.org/2001/XMLSchema#language"
    >es</m:idioma>
    <m:fecha rdf:datatype="http://www.w3.org/2001/XMLSchema#gYear"
