  <m:MasterThesis rdf:about="http://zaguan.unizar.es/record/1905">
    <m:titulo>Análisis, modelización y predicción de episodios de sequía</m:titulo>
    <rdf:type rdf:nodeID="A0"/>
    <m:fecha rdf:datatype="http://www.w3.org/2001/XMLSchema#gYear"
    >2014</m:fecha>
    <m:formato rdf:resource="http://github.com/vpec/Recoreon/skos#pdf"/>
