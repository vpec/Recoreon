@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix m: <http://github.com/vpec/Recoreon/> .

<m:Documento>
	a					rdfs:Class;
	rdfs:label			"Documento";
	rdfs:comment		"Documento de la coleccion Zaguan" .
	
	
<m:BachelorThesis>
	a					rdfs:Class;
	rdfs:label			"BachelorThesis";
	rdfs:subClassOf		<m:Documento>;
	rdfs:comment		"Bachelor Thesis" .
	
	
<m:MasterThesis>
	a					rdfs:Class;
	rdfs:label			"MasterThesis";
	rdfs:subClassOf		<m:Documento>;
	rdfs:comment		"Master Thesis" .
	

<m:Persona>
	a					rdfs:Class;
	rdfs:label			"Persona";
	rdfs:comment		"Persona" .


<m:nombrePersona>
	a					rdf:Property;
	rdfs:label			"NombrePersona";
	rdfs:domain  		<m:Persona>;
	rdfs:comment		"Nombre de una persona";
	rdfs:range			<rdfs:Literal> .
	

<m:Organizacion>
	a					rdfs:Class;
	rdfs:label			"Organizacion";
	rdfs:comment		"Organizacion" .
	

<m:nombreOrganizacion>
	a					rdf:Property;
	rdfs:label			"NombreOrganizacion";
	rdfs:domain			<m:Organizacion>;
	rdfs:comment		"Nombre de una organizacion";
	rdfs:range			<rdfs:Literal> .

	
<m:creador>
	a					rdf:Property;
	rdfs:label			"Autor";
	rdfs:domain  		<m:Documento>;
	rdfs:range			<m:Persona>;
	rdfs:comment		"Creador del trabajo" .


<m:titulo>
	a					rdf:Property;
	rdfs:label			"Titulo";
    rdfs:domain  		<m:Documento>;
	rdfs:range			<rdfs:Literal>;
	rdfs:comment		"Titulo del trabajo" .


<m:identificador>
	a					rdf:Property;
	rdfs:label			"Identificador";
	rdfs:domain  		<m:Documento>;
	rdfs:range			<rdfs:Literal>;
	rdfs:comment		"Identificador del trabajo" .


<m:tema>
	a					rdf:Property;
	rdfs:label			"Tema";
	rdfs:domain  		<m:Documento>;
	rdfs:range			<skos:Concept>;
	rdfs:comment		"Tema del trabajo" .


<m:publicador>
	a					rdf:Property;
	rdfs:label			"Publicador";
    rdfs:domain  		<m:Documento>;
	rdfs:range  		<m:Organizacion>;
	rdfs:comment		"Publicador del trabajo" .


<m:descripcion>
	a					rdf:Property;
	rdfs:label			"Descripcion";
	rdfs:domain  		<m:Documento>;
	rdfs:range			<rdfs:Literal>;
	rdfs:comment		"Descripcion del trabajo" .


<m:formato>
	a					rdf:Property;
	rdfs:label			"Formato";
	rdfs:domain  		<m:Documento>;
	rdfs:range			<skos:Concept>;
	rdfs:comment		"Formato del trabajo" .


<m:idioma>
	a					rdf:Property;
	rdfs:label			"Idioma";
	rdfs:domain  		<m:Documento>;
	rdfs:range			<xsd:language>;
	rdfs:comment		"Idioma del trabajo" .


<m:fecha>
	a					rdf:Property;
	rdfs:label			"Fecha";
	rdfs:domain  		<m:Documento>;
	rdfs:range			<xsd:gYear>;
	rdfs:comment		"Fecha del trabajo" .


<m:derechos>
	a					rdf:Property;
	rdfs:label			"Derechos";
	rdfs:domain  		<m:Documento>;
	rdfs:range			<skos:Concept>;
	rdfs:comment		"Derechos del trabajo" .



